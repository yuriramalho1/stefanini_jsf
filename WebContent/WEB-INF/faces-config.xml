<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">

	<converter>
		<converter-id>entityConverter</converter-id>
		<converter-class>converters.EntityConverter</converter-class>
	</converter>
	
	<validator>
		<validator-id>cpfValidator</validator-id>
		<validator-class>validator.CpfValidator</validator-class>
	</validator>
	
	<validator>
		<validator-id>validadorEmail</validator-id>
		<validator-class>validator.EmailValidator</validator-class>
	</validator>




</faces-config>
